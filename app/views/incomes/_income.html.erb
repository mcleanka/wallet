<div id="<%= dom_id income %>">
  <p>
    <strong>Title:</strong>
    <%= income.title %>
  </p>

  <p>
    <strong>Amount:</strong>
    <%= income.amount %>
  </p>

  <p>
    <strong>Funder name:</strong>
    <%= income.funder_name %>
  </p>

  <p>
    <strong>Source:</strong>
    <%= income.income_source.name %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= income.status.name %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= income.date %>
  </p>

</div>
